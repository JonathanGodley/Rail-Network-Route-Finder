public class Edge{ 
 
    private String name; 
    private String line; 
    private int duration;
 
    //Overloaded constructor 
    public Edge(String ename, String eline, int eduration){ 
        this.name = ename; 
        this.line = eline; 
        this.duration = eduration;
    } 
 
    public String get_name(){ 
        return this.name; 
    } 
 
    public String get_line(){ 
        return this.line; 
    } 

    public int get_duration(){
        return this.duration;
    }
 
}